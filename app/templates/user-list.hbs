<table class="table table-bordered">
  <thead>
    <tr>
      <th>Full Name</th>
      <th>username</th>
      <th>password</th>
    </tr>
  </thead>
  <tbody>
    {{#each model as |user| }}
      <tr>
        <td>{{user.fullName}}</td>
        <td>{{user.username}}</td>
        <td>{{user.password}}</td>
      </tr>   
    {{/each}}
  </tbody>
</table>